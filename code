    return(mnadd);
};
var mb=true;
var mn=0;
var mn0=[0];
var dezero=function(a){
    mn=0;
    mn0=[0];
    mb=false;
    for(i=0;i<a.length;i++){
        if(a[i]!==0){
            mb=true;
        }
        if(mb){
            mn++;
        }
    }
    for(i=0;i<mn;i++){
        mn0[mn-i]=a[a.length-i];
    }
    return(mn0);
};
var multiply=function(a,b){
    mnmult2=[0];
    if(a.length>b.length){
        for(i=0;i<b.length-1;i++){
            for(j=0;j<a.length-1;j++){
                mnmult2=add(mnmult2,shiftdecimal(toarray(a[j]*b[i]),j));
            }
        }
    }else{
        for(i=0;i<a.length-1;i++){
            for(j=0;j<b.length-1;j++){
                mnmult2=add(mnmult2,shiftdecimal(toarray(a[i]*b[j]),j));
            }
        }
    }
    return(mnmult2);
};
var printout=function(out){
    tout="";
    if(out[out.length-1]===0&&out!==[0]){
        for(i=0;i<out.length-1;i++){
            tout+=out[out.length-i-2];
        }
    }else{
        for(i=0;i<out.length;i++){
            tout+=out[out.length-i-1];
        }
    }
    console.log(tout);
};
var exponent=function(base,expo){
    mnexp=base;
    for(i=0;i<expo-1;i++){
        mnexp=multiply(mnexp,base);
    }
    return(mnexp);
};
printout(add(n1,n2));
printout(add([0,0,1],[5,4]));
printout(add([5,4],[0,0,1]));
