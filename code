var n1=[10000],n2=[6,5,2],n3=[],n4=[],m5=[],mn=0,nm=0,mnadd=[],mnadd1=[],mnmult1=[],mnmult2=[],mnmult3=[],mndiv=[],mnexp=[],i=0,j=0,tadd="",tmult="",texp="",tout="",binaryCode,midnum,midarray=[],midshift=[];
var toBinary = function(number) {
    binaryCode = [];
    while (number > 0) {
        binaryCode[binaryCode.length] = number % 2;
        number >>= 1;
    }
    return(binaryCode);
};
var toDecimal=function(bin){
    midnum=0;
    for(i=0;i<bin.length;i++){
        midnum+=bin[i]*pow(2,i);
    }
    return(midnum);
};
var toarray=function(a){
    midarray=[];
    //if(a>9){
        for(i=0;i<Math.ceil(Math.log(a)/Math.log(10));i++){
            midarray[i]=Math.floor(a/Math.pow(10,i))-Math.floor(a/Math.pow(10,i+1))*10;
        }
    //}else{
        //midarray=[a];
    //}
    return(midarray);
};
var shiftdecimal=function(a,n){
    midshift=[];
    for(i=0;i<a.length;i++){
        midshift[i+n]=a[i];
    }
    for(i=0;i<n;i++){
        midshift[i]=0;
    }
    return(midshift);
};
var add=function(a,b){
    mnadd=[0];
    /*if(a.length>b.length){
